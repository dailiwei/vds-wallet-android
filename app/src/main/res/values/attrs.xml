<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberPickerView">
        <attr name="align">
            <enum name="CENTER" value="1" />
            <enum name="LEFT" value="0" />
            <enum name="RIGHT" value="2" />
        </attr>
        <attr name="dividerColor" format="reference|color"/>
        <attr name="dividerHeight" format="reference|dimension"/>
        <attr name="dividerMarginLeft" format="reference|dimension"/>
        <attr name="dividerMarginRight" format="reference|dimension"/>
        <attr name="fadingEdgeEnabled" format="boolean"/>
        <attr name="max" format="integer"/>
        <attr name="min" format="integer"/>
        <attr name="selectedTextColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="typeface" format="string"/>
        <attr name="wheelItemCount" format="integer"/>
        <attr name="wrapSelectorWheel" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="SearchViewGroup">
        <attr name="background" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ChatHeader">
        <attr name="title" format="string"/>
    </declare-styleable>
</resources>